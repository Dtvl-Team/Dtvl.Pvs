@model PvOption<DtvlPvs>
@{
    Model.WithParentTag("v-tabs-window-item")
        .FillSlotFromDefault(TabContentSlot.Content)
        .AddParentAttr(new
        {
            @class = "TabContent",
        });

    if (Model.TryGetSlotContent(TabContentSlot.Value, out var Value))
        Model.AddParentAttr("value", Value);
}

@if (Model.TryGetSlotRender(TabContentSlot.Content, out var ContentRender))
{
    @ContentRender
}